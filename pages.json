{
	"easycom": {
		// npm安装的方式不需要前面的"@/"，下载安装的方式需要"@/"
		"^uni-(.*)": "@/uni_modules/uni-$1/components/uni-$1/uni-$1.vue",
		"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				// "navigationBarTitleText": "阅卷分析"
			}
		}, {
			"path": "pages/reportDetail/reportDetail",
			"style": {
				// "navigationBarTitleText": "具体分析",
				"enablePullDownRefresh": true
			}

		}, {
			"path": "pages/my/my",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "pages/tool/tool",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "pages/tool/map",
			"style": {
				"navigationBarTitleText": "阅卷分析-地图",
				"enablePullDownRefresh": false
			}
		}, {
			"path": "pages/tool/charts",
			"style": {
				"navigationBarTitleText": "图表",
				"enablePullDownRefresh": false
			}
		}
	],
	"tabBar": {
		"color": "#7A7E83",
		"height": "50px",
		"selectedColor": "#1cbc9c",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"list": [{
			"pagePath": "pages/index/index",
			"iconPath": "static/images/home.png",
			"selectedIconPath": "static/images/homeSelect.png",
			"text": "首页"
		}, {
			"pagePath": "pages/tool/tool",
			"iconPath": "static/images/tool.png",
			"selectedIconPath": "static/images/toolSelect.png",
			"text": "功能"
		}, {
			"pagePath": "pages/my/my",
			"iconPath": "static/images/me.png",
			"selectedIconPath": "static/images/meSelect.png",
			"text": "我的"
		}]
	},
	"globalStyle": {
		"navigationBarTitleText": "阅卷分析",
		"navigationBarBackgroundColor": "#1CBC9C",
		"backgroundColor": "#F8F8F8",
		"navigationBarTextStyle": "white",
		"animationType": "fade-in"
	}
}
